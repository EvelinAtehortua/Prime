                    @*@Html.Raw(ViewBag.Menu)*@
                    @*</li>*@
                </ul>
            </nav>
            <nav>
                <h5 class="sidebar-header">Menú</h5>
                <ul class="nav nav-pills nav-stacked">
                    @*<li><a href="#" title="Menú"><i class='fa  fa-fw fa-list-alt'></i>Menú</a>*@
                    @Html.Raw(System.Web.HttpContext.Current.Session["Menu"])
                    @*</li>*@
                </ul>
            </nav>
        </aside>
        <section class="main-content-wrapper">
            @*<div class="breadcrumbs ace-save-state" id="breadcrumbs">
                <ul class="breadcrumb">
                    <li>
                        <i class="ace-icon fa fa-home home-icon"></i>
                        <a href="../Menu/Menu">Principal</a>
                    </li>
                </ul>
            </div>*@
            @if (!string.IsNullOrEmpty(ViewBag.Parametros))
            {
                <div id="config" class="config">
                    <h4>PARAMETROS<a href="javascript:void(0)" class="config-link closed"><i class="fa fa-indent"></i></a></h4>
                    <div class="config-swatch-wrap">
                        <div class="row">
                            <form id="formParam" action="../Reporte/cargar" method="POST">
                                <ul class="list-group scrollable-menu">
                                    @Html.Raw(ViewBag.Parametros)
                                    <li class="list-group-item"><button type="button" class="btn btn-primary" id="cargar">Cargar</button></li>
                                </ul>
                            </form>
                        </div>
                    </div>
                </div>
            }
            <section id="main-content">
                @RenderBody()
            </section>
        </section>
        @*<div class="navbar navbar-default navbar-fixed-bottom">
            <div style="text-align:center">
                <footer>
                    <img src="~/Images/LogoAlargadoTransparente.png" />
                </footer>
            </div>
        </div>*@
    </section>
    
    @*<div class="navbar navbar-default bottom">
        <div style="text-align:center">
            <footer>
                <img src="~/Images/LogoAlargadoTransparente.png" />
            </footer>
        </div>
    </div>*@
    <script src="~/scripts/jquery-1.9.1.min.js"></script>
    @*<script src="~/scripts/jquery-1.9.1.js"></script>*@
    <script src="~/scripts/bootstrap.min.js"></script>
    <script src="~/scripts/jquery.navgoco.min.js"></script>
    <script src="~/scripts/app.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.3.0/js/bootstrap-datepicker.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.17.37/js/bootstrap-datetimepicker.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/select2/3.4.8/select2.min.js"></script>
    <script src="~/scripts/raphael-2.1.4.min.js"></script>
    <script src="~/scripts/justgage.js"></script>
    <script src="https://code.highcharts.com/highcharts.js"></script>
    <script type="text/javascript">

        $(document).ready(function () {

            $('#cargar').click(function () {
                var dimVal = [];
                //var tiempo = [];

                @foreach (var item in dim)
                {
                    if(item.tipoControl.Equals("checkbox"))
                    {
                        @: obtenerCheck('@item.idControl', '@item.nombreDimension', dimVal)
                    }
                    if (item.tipoControl.Equals("select"))
                    {
                        @: obtenerSelect('@item.idControl', '@item.nombreDimension', dimVal)
                                    }
                    if (item.tipoControl.Equals("radiobutton"))
                    {
                        @: obtenerRadio('@item.idControl', '@item.nombreDimension', dimVal)
                                    }
                    if (item.tipoControl.Equals("textarea"))
                    {
                        @: obtenerTextarea('@item.idControl', '@item.nombreDimension', dimVal)
                                    }
                    if(item.tipoControl.Equals("date") || item.tipoControl.Equals("text"))
                    {
                        @: obtenerText('@item.idControl', '@item.nombreDimension', dimVal)
                    }
                }
                //if (tiempo.length > 0) {
                //    dimVal.push("Tiempo");
                //    for (i = 0; i < tiempo.length; i++) {
                //        dimVal.push(tiempo[i]);
                //    }
                //    dimVal.push("|");
                //    tiempo = [];
                //}
                alert(dimVal.join(", "));
            });

            function obtenerCheck(id, dimension, dimVal) {
                dimVal.push(dimension);
                $.each($('.param'+id+':checkbox:checked'), function () {
                    dimVal.push($(this).val());
                });
                dimVal.push("|");
            }

            function obtenerSelect(id, dimension, dimVal) {
                dimVal.push(dimension);
                var valor = $('#param'+id+'').val();
                dimVal.push(valor);
                dimVal.push("|");
            }

            function obtenerRadio(id, dimension, dimVal) {
                dimVal.push(dimension);
                var valor = $('#param'+id+'').val();
                dimVal.push(valor);
                dimVal.push("|");
            }

            function obtenerTextarea(id, dimension, dimVal) {
                dimVal.push(dimension)
                var valor = $('#param'+id+'').val();
                if (valor == "") {
                    dimVal.push("null");
                } else {
                    dimVal.push(valor);
                }
                dimVal.push("|");
            }

            function obtenerText(id, dimension, dimVal) {
                dimVal.push(dimension);
                var valor = $('#param'+id+'').val();
                if (valor == "") {
                    dimVal.push("null");
                } else {
                    dimVal.push(valor);
                }
                dimVal.push("|");
            }


            //Generador de gauges encuesta
            var cara1, cara2, cara3, cara4, cara5, cara6, cara7;

            window.onload = function () {
                var cara1 = new JustGage({
                    id: "cara1",
                    value: getRandomInt(0, 100),
                    min: 0,
                    max: 100,
                    symbol: "%",
                    relativeGaugeSize: true,
                });

                var cara2 = new JustGage({
                    id: "cara2",
                    value: getRandomInt(0, 100),
                    min: 0,
                    max: 100,
                    symbol: "%",
                    relativeGaugeSize: true,
                });

                var cara3 = new JustGage({
                    id: "cara3",
                    value: getRandomInt(0, 100),
                    min: 0,
                    max: 100,
                    symbol: "%",
                    relativeGaugeSize: true,
                });

                var cara4 = new JustGage({
                    id: "cara4",
                    value: getRandomInt(0, 100),
                    min: 0,
                    max: 100,
                    symbol: "%",
                    relativeGaugeSize: true,
                });

                var cara5 = new JustGage({
                    id: "cara5",
                    value: getRandomInt(0, 100),
                    min: 0,
                    max: 100,
                    symbol: "%",
                    relativeGaugeSize: true,
                });

                var cara6 = new JustGage({
                    id: "cara6",
                    value: getRandomInt(0, 100),
                    min: 0,
                    max: 100,
                    symbol: "%",
                    relativeGaugeSize: true,
                });

                var cara7 = new JustGage({
                    id: "cara7",
                    value: getRandomInt(0, 100),
                    min: 0,
                    max: 100,
                    symbol: "%",
                    relativeGaugeSize: true,
                });

                setInterval(function () {
                    cara1.refresh(getRandomInt(50, 100));
                    cara2.refresh(getRandomInt(50, 100));
                    cara3.refresh(getRandomInt(50, 100));
                    cara4.refresh(getRandomInt(50, 100));
                    cara5.refresh(getRandomInt(50, 100));
                    cara6.refresh(getRandomInt(50, 100));
                    cara7.refresh(getRandomInt(50, 100));
                }, 2500);
            };

            $(function () {
                $('#container').highcharts({
                    /*title: {
                        text: 'Monthly Average Temperature',
                        x: -20 //center
                    },
                    subtitle: {
                        text: 'Source: WorldClimate.com',
                        x: -20
                    },*/
                    xAxis: {
                        categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun',
                            'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec']
                    },
                    yAxis: {
                        title: {
                            text: 'Temperature (°C)'
                        },
                        plotLines: [{
                            value: 0,
                            width: 1,
                            color: '#808080'
                        }]
                    },
                    tooltip: {
                        valueSuffix: '°C'
                    },
                    series: [{
                        name: 'Tokyo',
                        data: [7.0, 6.9, 9.5, 14.5, 18.2, 21.5, 25.2, 26.5, 23.3, 18.3, 13.9, 9.6]
                    }, {
                        name: 'New York',
                        data: [-0.2, 0.8, 5.7, 11.3, 17.0, 22.0, 24.8, 24.1, 20.1, 14.1, 8.6, 2.5]
                    }, {
                        name: 'Berlin',
                        data: [-0.9, 0.6, 3.5, 8.4, 13.5, 17.0, 18.6, 17.9, 14.3, 9.0, 3.9, 1.0]
                    }, {
                        name: 'London',
                        data: [3.9, 4.2, 5.7, 8.5, 11.9, 15.2, 17.0, 16.6, 14.2, 10.3, 6.6, 4.8]
                    }]
                });
            });

            @if (System.Web.HttpContext.Current.Session["paramDates"] != null)
            {
                List<int> d = (List<int>)Session["paramDates"];

                for (int index = 0; index < d.Count; index++)
                {
                    @: cargarCalendario('@d[index]')
                }
            }
            @if (System.Web.HttpContext.Current.Session["paramSelects"] != null)
            {
                List<int> s = (List<int>)Session["paramSelects"];

                for (int index = 0; index < s.Count; index++)
                { 
                    @: cargarCombo('@s[index]')
                }
            }

            function cargarCalendario(id) {
                $('#param' + id + '').datepicker({
                    format: 'mm-dd-yyyy'
                });
            }

            function cargarCombo(id) {
                $('#param' + id + '').select2();
            }
        });
    </script>
</body>
</html>
