<body>
    @{
        List<Prime_Web.Models.Dimension> dim = new List<Prime_Web.Models.Dimension>();
        if (System.Web.HttpContext.Current.Session["dimensiones"] != null)
        {
            dim = (List<Prime_Web.Models.Dimension>)System.Web.HttpContext.Current.Session["dimensiones"];
        }
    }
    <!-- Page Wrapper-->
    <section id="main-wrapper" class="theme-default">
        <!-- Top Header -->
        <header id="header">
            <ul class="nav navbar-nav navbar-left">
                <li class="toggle-navigation toggle-left">
